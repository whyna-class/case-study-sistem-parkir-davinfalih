generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Parkir {
  id             Int        @id @default(autoincrement())
  plat_nomor     String     @db.VarChar(50)
  jenis_kendaraan JenisKendaraan
  durasi         Int
  total          Int
  createdAt      DateTime   @default(now())
  updatedAt      DateTime   @updatedAt

  @@map("parkir")
}

enum JenisKendaraan {
  roda2
  roda4
}
